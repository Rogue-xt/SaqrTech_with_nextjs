generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url      = env("DATABASE_URL")
}

model VanSalesTrial {
  id          String   @id @default(cuid())
  createdAt   DateTime @default(now())
  name        String
  number      String
  email       String
  tallyUser   String
  tallynumber String?  // Optional field
  info        String?
}
